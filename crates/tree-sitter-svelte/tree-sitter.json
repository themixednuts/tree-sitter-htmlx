{
  "grammars": [
    {
      "name": "svelte",
      "camelcase": "Svelte",
      "scope": "source.svelte",
      "path": ".",
      "external-files": [
        "src/htmlx/html/tag.h",
        "src/htmlx/html/scanner.c",
        "src/htmlx/scanner.c"
      ],
      "file-types": [
        "svelte"
      ],
      "highlights": [
        "queries/htmlx/html/highlights.scm",
        "queries/htmlx/highlights.scm",
        "queries/highlights.scm"
      ],
      "injections": [
        "queries/htmlx/injections.scm",
        "queries/injections.scm"
      ],
      "injection-regex": "svelte"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Svelte 5 grammar for tree-sitter"
  },
  "bindings": {
    "rust": true
  }
}
